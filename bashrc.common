# Any bashrc settings for a standard environment

export VISUAL=nvim
export EDITOR="$VISUAL"

# $HOME/bin/custom_bin symlinked to dotfiles bin/
export PATH=$HOME/bin/custom_bin:$PATH

if [ -f ~/.bash_aliases ]; then
    . ~/.bash_aliases
fi

if [ -f ~/.bash_functions ]; then
    . ~/.bash_functions
fi
